{
  "version": 2,
  "name": "solar-predictor",
  "builds": [
    {
      "src": "static_web/**",      // 构建static_web目录下的所有文件
      "use": "@vercel/static"      // 使用静态文件构建器
    }
  ],
  "routes": [
    {
      "src": "/(.*)",             // 匹配所有路由
      "dest": "/static_web/$1"    // 重定向到static_web目录
    }
  ]
}
